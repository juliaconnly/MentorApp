<!doctype html>

<html>
<head>
  <title>HCI Project Portfolio</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="/css/bootstrap.min.css" rel="stylesheet">
  <link href="/css/bootstrap-theme.min.css" rel="stylesheet">
  <link href="/css/introHCI.css" rel="stylesheet">
  <!-- this is a comment in HTML -->

</head>

<body>
  <div class="jumbotron" id="indexJumbo">
    <a href="/"><img class="img" id="logoImg"></a>
    <center><h4>MentorMe</h4></center>
    <center><p>Find a Mentor &middot; Find a Mentee </p></center>
  </div>

   <div class="container">
    <center><img src="../images/messageIcon.png" alt="Message image" class="mailimg_main"></center>

  {{#each messagesList}}
  <div class="project clearfix" id={{name}}>
    <a href="viewmessages/{{name}}" class="thumbnail" id="inbox">
      <img src="{{image}}" class="profilepic" style="float:left">
      <p id="messageName">{{name}}</p>
      <p id="messageM">{{message}}</p>
    </a>
  </div>
  {{/each}}

</div>
</div>

<!-- NAV STARTS HERE -->
<div id="nav">

<li><img src="../images/backarrow.png" alt="Back image" class="backimg"></li>
<li><a href="/"><img src="../images/homeicon.png" alt="Home image" class="homeimg"></a></li>
<li><a href=""><img src="../images/messageIcon.png" alt="Message image" class="mailimg"></a></li>
<li><a href="../settings/"><img src="../images/settings.png" alt="Settings image" class="settingsimg"></a></li>
<li><a href="../FAQ/">FAQ</a></li>

</div>
<!-- NAV ENDS HERE -->


  <script src="https://code.jquery.com/jquery.js"></script>
  <script src="/js/bootstrap.min.js"></script>
